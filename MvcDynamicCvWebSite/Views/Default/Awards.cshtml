@using MvcDynamicCvWebSite.Models.Entity
@model List<Awards>

<div class="resume-section-content">
    <h2 class="mb-5">Sertifikalarım</h2>
    <ul class="fa-ul mb-0">
        @foreach (var item in Model)
        {
            <li>
                <span class="fa-li"><i class="fas fa-trophy text-warning"></i></span>
                 @item.Description
            </li>
         } 
    </ul>
</div>